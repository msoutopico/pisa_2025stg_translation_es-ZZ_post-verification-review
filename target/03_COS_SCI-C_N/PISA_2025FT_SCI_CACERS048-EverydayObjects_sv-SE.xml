            <label key="64702130404e38.89447078_5fdaef2afe66082403304e60d189c0e4_1">
                <text>Vardagsföremål</text>
            </label>
            <label key="64702130404e38.89447078_93d396dd95885c71d2c0eb849df1edae_2">
                <text>Detta är några vardagsföremål.</text>
            </label>
            <label key="64702130404e38.89447078_fbf4c1431830897eef3d4546ddc74013_3">
                <text><img alt="CACERS048S1_everyday-objects_1.svg" src="a32e99438bc978c6f78e41a028f25f84ac577ed7f55343a60f15c862db08d607" width="99%"/>‌Metallskruv </text>
            </label>
            <label key="64702130404e38.89447078_0024ea8f902cbe61707fcf74f5e0aa31_4">
                <text><img alt="CACERS048S1_everyday-objects_2.svg" src="d184317376972b3ca19a6b4816f84178ef4082cc1e3da811e1cbd201c1144e79" width="99%"/>‌Plastsked </text>
            </label>
            <label key="64702130404e38.89447078_9eb2f30cbe8ffd4781280e9158a75b90_5">
                <text><img alt="CACERS048S1_everyday-objects_3.svg" src="03b80337ad9d5db48450b62beeb25fba62f924a78a8b8590d4bdeebd2a52a369" width="99%"/>‌Träskål </text>
            </label>
            <label key="64702130404e38.89447078_c04dee16cce9f1bc857468037a628b4d_6">
                <text><img alt="CACERS048S1_everyday-objects_4.svg" src="7f6adb652e90344c671cf9545512726d705c9eec0675b939b4905ae76170185e" width="99%"/>‌ Papperspåse </text>
